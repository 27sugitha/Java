public class LargestInArray {
    public static void main(String[] args) {
        int[] numbers = {12, 45, 67, 89, 23, 90, 77};
        int max = numbers[0];

        for (int num : numbers) {
            if (num > max) {
                max = num;
            }
        }

        System.out.println("Largest number: " + max);
    }
}
